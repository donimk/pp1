#include <iostream>
#include <iomanip>  
using namespace std;

int main() {
    float weight, height;

    cout << "Enter your weight in kg: ";
    cin >> weight;

    cout << "Enter your height in meters: ";
    cin >> height;

    float bmi = weight / (height * height);

    cout << fixed << setprecision(2);
    cout << "Your BMI is " << bmi << endl;

    return 0;
}
